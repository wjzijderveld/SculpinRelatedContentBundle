{
    "name": "wjzijderveld/sculpin-related-content-bundle",
    "type": "symfony-bundle",
    "keywords": ["sculpin", "related", "content", "static", "bundle"],
    "description": "Sculpin Bundle to generate related content data",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem-Jan Zijderveld",
            "email": "wjzijderveld@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/config": "~2.1",
        "symfony/dependency-injection": "~2.1",
        "symfony/http-kernel": "~2.1",
        "sculpin/sculpin": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Wjzijderveld\\Sculpin\\RelatedContentBundle": ""
        }
    },
    "target-dir": "Wjzijderveld/Sculpin/RelatedContentBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
